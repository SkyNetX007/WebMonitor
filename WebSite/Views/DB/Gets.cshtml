@{
    ViewData["Title"] = "Gets";
}

<h1>数据列表</h1>

<table class="table">
    <tr>
        <th>ID</th>
        <th>TIME</th>
        <th>DIAMETER</th>
        <th>POS</th>
        <th>PASSED</th>
    </tr>
    @{
        List<List<string>> tableResult = (List<List<string>>)ViewData["tableResult"];
        foreach (List<string> l in tableResult)
        {
            <tr>
                @{
                    foreach (string s in l)
                    {
                        <td>@s</td>
                    }
                }
            </tr>
        }
    }
</table>

<script type="text/javascript">
setInterval(function() {
  location.reload();
}, 3000);
</script>